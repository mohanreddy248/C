#include <stdio.h>

int main() {
    int blocks, stars;

    printf("Enter the number of blocks: ");
    scanf("%d", &blocks);

    printf("Enter the number of stars: ");
    scanf("%d", &stars);

    for(int i = 0; i < blocks; i++) {
        for(int j = 0; j < stars - i; j++) {
            printf("*****\n");
        }
        printf("\n"); // Separate each block
    }

    return 0;
}
